    "yarn": ">=1.15.*"
  },
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "antd": "^4.6.6",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.15",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.3.4",
    "axios-mock-adapter": "^1.18.1",
    "babel-preset-minify": "^0.5.1",
    "bcrypt": "^5.0.0",
    "chalk": "^4.0.0",
    "formik": "^2.2.6",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^6.6.1",
    "nodemon": "^2.0.4",
    "react-redux": "^7.2.2",
    "supertest": "^4.0.2",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start-client": "react-scripts start",
    "build-client": "react-scripts build",
    "test-client": "react-scripts test --coverage --watchAll=false",
    "serve": "cd build-server && node server.js",
    "start-server": "nodemon --exec babel-node server/server.js",
    "test-server": "jest --config=server/jest.config.js /server --coverage",
    "build-server": "babel ./server --out-dir build-server --ignore \"server/coverage\",\"server/**/*.test.js\",\"server/jest.config.js\" --presets minify",
    "seed": "babel-node tools/seedDatabase.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
